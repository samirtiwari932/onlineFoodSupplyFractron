{
    "version": 2,
    "functions": {
        "server/src/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/src/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/client/dist/$1"
        }
    ]
}