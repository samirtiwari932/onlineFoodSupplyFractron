{
    "version": 2,
    "outputDirectory": "client/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/src/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/client/dist/assets/$1"
        },
        {
            "source": "/vite.svg",
            "destination": "/client/dist/vite.svg"
        },
        {
            "source": "/(.*)",
            "destination": "/client/dist/index.html"
        }
    ]
}